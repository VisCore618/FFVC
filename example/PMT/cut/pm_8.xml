<?xml version="1.0" encoding="UTF-8"?>
<SphereConfig SolverType="CBC">
    <DomainInfo>
        <VoxelOrigin ox="0.0" oy="0.0" oz="0.0"/>
        <VoxelSize ix="256" jx="256" kx="256"/>
        <VoxelPitch dx="1.0e-02" dy="1.0e-02" dz="1.0e-02"/>
    </DomainInfo>
    <Steer>
        <Elem name="Algorithm">
            <Param dtype="STRING" name="Flow" value="FS_C_EE_D_EE"/>
            <Param dtype="STRING" name="Heat" value="C_EE_D_EE"/>
        </Elem>
        <Elem name="Average_option">
            <Param dtype="STRING" name="Operation" value="Off"/>
            <Param dtype="STRING" name="Start_Type" value="Time"/>
            <Param dtype="REAL" name="Start" value="500.0"/>
        </Elem>
        <Param dtype="STRING" name="Check_Parameter" value="Off"/>
        <Elem name="Convection_Term">
            <Param dtype="STRING" name="Scheme" value="O3_MUSCL"/>
            <Param dtype="STRING" name="Limiter" value="Minmod"/>
        </Elem>
        <Elem name="Derived_Variable">
            <Param dtype="STRING" name="Total_Pressure" value="Off"/>
            <Param dtype="STRING" name="Helicity" value="Off"/>
            <Param dtype="STRING" name="Vorticity" value="Off"/>
            <Param dtype="STRING" name="2nd_Invariant_of_VGT" value="Off"/>
        </Elem>
        <Param dtype="STRING" name="Example" value="Performance_Test"/>
        <Elem name="File_IO">
            <Param dtype="STRING" name="Unit_of_File" value="Non_Dimensional"/>
            <Param dtype="STRING" name="Guide_Out" value="Without"/>
            <Param dtype="STRING" name="Output_Mode" value="Forced"/>
            <Param dtype="STRING" name="Parallel_Input" value="Master"/>
            <Param dtype="STRING" name="Parallel_Output" value="Master"/>
            <Param dtype="STRING" name="Debug_Divergence" value="Off"/>
            <Param dtype="STRING" name="Instant_Interval_Type" value="Step"/>
            <Param dtype="REAL" name="Instant_Interval" value="1000"/>
            <Param dtype="STRING" name="Averaged_Interval_Type" value="Time"/>
            <Param dtype="REAL" name="Averaged_Interval" value="1000"/>
        </Elem>
        <InputData basedir=".">
            <InFile attr="SphereSVX" fname="example.svx" format="SVX"/>
        </InputData>
        <Elem name="Iteration">
            <Elem name="Flow">
                <Elem name="Poisson">
                    <Param dtype="INT" name="Iteration" value="20"/>
                    <Param dtype="REAL" name="Epsilon" value="1.0e-3"/>
                    <Param dtype="REAL" name="Omega" value="1.2"/>
                    <Param dtype="STRING" name="Norm" value="v_div_max"/>
                    <Param dtype="STRING" name="Linear_Solver" value="SOR2SMA"/>
                </Elem>
            </Elem>
            <Elem name="Heat">
                <Elem name="Euler_Implicit">
                    <Param dtype="INT" name="Iteration" value="30"/>
                    <Param dtype="REAL" name="Epsilon" value="1.0e-2"/>
                    <Param dtype="REAL" name="Omega" value="1.2"/>
                    <Param dtype="STRING" name="Norm" value="T_Res_L2_Absolute"/>
                    <Param dtype="STRING" name="Linear_Solver" value="SOR"/>
                </Elem>
            </Elem>
        </Elem>
        <Elem name="LES_Option">
            <Param dtype="STRING" name="LES_Calculation" value="Off"/>
        </Elem>
        <Elem name="Log">
            <Param dtype="STRING" name="Unit_of_Log" value="Non_Dimensional"/>
            <Param dtype="STRING" name="Log_Base" value="On"/>
            <Param dtype="STRING" name="Log_Iteration" value="Off"/>
            <Param dtype="STRING" name="Log_Profiling" value="detail"/>
            <Param dtype="STRING" name="Log_Wall_Info" value="Off"/>
            <Param dtype="STRING" name="Console_Interval_Type" value="Step"/>
            <Param dtype="REAL" name="Console_Interval" value="1"/>
            <Param dtype="STRING" name="History_Interval_Type" value="Step"/>
            <Param dtype="REAL" name="History_Interval" value="1"/>
        </Elem>
        <Elem name="Model_Setting">
            <Param comment="air" dtype="INT" id="1" name="Fluid" value="800"/>
            <Param comment="wall" dtype="INT" id="600" name="Solid" value="600"/>
        </Elem>
        <Elem name="Monitor_List">
            <Param dtype="STRING" name="Log" value="Off"/>
            <Param dtype="STRING" name="Output_File" value="sample.log"/>
            <Param dtype="STRING" name="Output_Mode" value="Gather"/>
            <Param dtype="STRING" name="Unit" value="Non_Dimensional"/>
            <Param dtype="STRING" name="Sampling_Interval_Type" value="Time"/>
            <Param dtype="REAL" name="Sampling_Interval" value="0.1"/>
            <Elem comment="y=0_x" name="Line">
                <Param dtype="STRING" name="Variable" value="Velocity"/>
                <Param dtype="STRING" name="Sampling_Method" value="Interpolation"/>
                <Param dtype="STRING" name="Sampling_Mode" value="Fluid"/>
                <Param dtype="INT" name="Division" value="64"/>
                <Elem name="From">
                    <Param dtype="REAL" name="X" value="-0.5"/>
                    <Param dtype="REAL" name="Y" value="0.0"/>
                    <Param dtype="REAL" name="Z" value="0.0"/>
                </Elem>
                <Elem name="To">
                    <Param dtype="REAL" name="X" value="0.5"/>
                    <Param dtype="REAL" name="Y" value="0.0"/>
                    <Param dtype="REAL" name="Z" value="0.0"/>
                </Elem>
            </Elem>
            <Elem comment="y=0_z" name="Line">
                <Param dtype="STRING" name="Variable" value="Velocity"/>
                <Param dtype="STRING" name="Sampling_Method" value="Interpolation"/>
                <Param dtype="STRING" name="Sampling_Mode" value="Fluid"/>
                <Param dtype="INT" name="Division" value="64"/>
                <Elem name="From">
                    <Param dtype="REAL" name="X" value="0.0"/>
                    <Param dtype="REAL" name="Y" value="0.0"/>
                    <Param dtype="REAL" name="Z" value="-0.5"/>
                </Elem>
                <Elem name="To">
                    <Param dtype="REAL" name="X" value="0.0"/>
                    <Param dtype="REAL" name="Y" value="0.0"/>
                    <Param dtype="REAL" name="Z" value="0.5"/>
                </Elem>
            </Elem>
        </Elem>
        <OutputData basedir=".">
            <OutFile attr="Velocity" basename="vel" format="SPH" interval="1"/>
            <OutFile attr="Pressure" basename="prs" format="SPH" interval="1"/>
            <OutFile attr="Temperature" basename="tmp" format="SPH" interval="1"/>
            <OutFile attr="AvrVelocity" basename="vela" format="SPH" interval="1"/>
            <OutFile attr="AvrPressure" basename="prsa" format="SPH" interval="1"/>
            <OutFile attr="AvrTemperature" basename="tmpa" format="SPH" interval="1"/>
        </OutputData>
        <Elem name="Reference_Frame">
            <Param dtype="STRING" name="Reference_Frame_Type" value="Stationary"/>
        </Elem>
        <StartCondition type="Initial"/>
        <Elem name="Solver_Property">
            <Param dtype="STRING" name="Basic_Equation" value="Incompressible"/>
            <Param dtype="STRING" name="Buoyancy" value="No_Buoyancy"/>
            <Param dtype="STRING" name="Kind_of_Solver" value="Flow_Only"/>
            <Param dtype="STRING" name="PDE_type" value="Navier_Stokes"/>
            <Param dtype="STRING" name="Shape_Approximation" value="cut_distance"/>
            <Param dtype="STRING" name="Time_Variation" value="Unsteady"/>
            <Param dtype="STRING" name="Pressure_Shift" value="off"/>
        </Elem>
        <Elem name="Time_Control">
            <Param dtype="STRING" name="Acceleration_Type" value="Time"/>
            <Param dtype="REAL" name="Acceleration" value="1.0"/>
            <Param dtype="STRING" name="Dt_Type" value="CFL_Reference_Velocity"/>
            <Param dtype="REAL" name="Delta_T" value="0.2"/>
            <Param dtype="STRING" name="Period_Type" value="Step"/>
            <Param dtype="REAL" name="Calculation_Period" value="20"/>
        </Elem>
        <Elem name="Treatment_of_Wall">
            <Param dtype="STRING" name="Pressure_Gradient" value="Grad_Zero"/>
            <Param dtype="STRING" name="Velocity_Profile" value="No_Slip"/>
        </Elem>
        <Elem name="Unit">
            <Param dtype="STRING" name="Unit_of_Input_Parameter" value="Non_Dimensional"/>
            <Param dtype="STRING" name="Pressure" value="Gauge"/>
            <Param dtype="STRING" name="Temperature" value="Celsius"/>
        </Elem>
        <Elem name="Version_Info">
            <Param dtype="INT" name="CBC" value="160"/>
            <Param dtype="INT" name="Flow_Base" value="256"/>
        </Elem>
    </Steer>
    <Parameter>
        <Elem name="Reference">
            <Param dtype="REAL" name="Length" value="1.0"/>
            <Param dtype="REAL" name="Velocity" value="1.0"/>
            <Param dtype="REAL" name="Gravity" value="9.8"/>
            <Param dtype="REAL" name="Base_Pressure" value="0.0"/>
            <Param dtype="REAL" name="Reynolds" value="1000.0"/>
            <Param dtype="REAL" name="Prandtl" value="0.71"/>
            <Param dtype="INT" name="Ref_ID" value="1"/>
        </Elem>
        <Elem name="Temperature">
            <Param dtype="REAL" name="Base" value="20.0"/>
            <Param dtype="REAL" name="Difference" value="35.0"/>
        </Elem>
        <Elem name="Initial_State">
            <Param dtype="REAL" name="Density" value="1.0"/>
            <Param dtype="REAL" name="Pressure" value="0.0"/>
            <Param dtype="REAL" name="Temperature" value="20.0"/>
            <Elem name="Velocity">
                <Param dtype="REAL" name="U" value="0.0"/>
                <Param dtype="REAL" name="V" value="0.0"/>
                <Param dtype="REAL" name="W" value="0.0"/>
            </Elem>
        </Elem>
    </Parameter>
    <Medium_Table>
        <Elem comment="Water" id="1" name="Fluid">
            <Param dtype="REAL" name="Density" value="998.2"/>
            <Param dtype="REAL" name="Specific_Heat" value="4182"/>
            <Param dtype="REAL" name="Thermal_Conductivity" value="598e-3"/>
            <Param dtype="REAL" name="Kinematic_Viscosity" value="1.004e-6"/>
            <Param dtype="REAL" name="Viscosity" value="1002.6e-06"/>
            <Param dtype="REAL" name="Sound_of_Speed" value="1500.0"/>
            <Param name="volume_expansion" dtype="REAL" value="0.04e-3" />
        </Elem>
        <Elem comment="Air" id="800" name="Fluid">
            <Param dtype="REAL" name="Density" value="1.1763"/>
            <Param dtype="REAL" name="Specific_Heat" value="1007"/>
            <Param dtype="REAL" name="Thermal_Conductivity" value="2.614e-02"/>
            <Param dtype="REAL" name="Kinematic_Viscosity" value="15.83e-06"/>
            <Param dtype="REAL" name="Viscosity" value="18.62e-06"/>
            <Param dtype="REAL" name="Sound_of_Speed" value="340.0"/>
            <Param name="volume_expansion" dtype="REAL" value="0.04e-3" />
        </Elem>
        <Elem comment="Fe" id="600" name="Solid">
            <Param dtype="REAL" name="Density" value="7870.0"/>
            <Param dtype="REAL" name="Specific_Heat" value="442.0"/>
            <Param dtype="REAL" name="Thermal_Conductivity" value="80.3"/>
        </Elem>
        <Elem comment="Al" id="500" name="Solid">
            <Param dtype="REAL" name="Density" value="2688.0"/>
            <Param dtype="REAL" name="Specific_Heat" value="905.0"/>
            <Param dtype="REAL" name="Thermal_Conductivity" value="237.0"/>
        </Elem>
    </Medium_Table>
    <BC_Table>
        <InnerBoundary/>
        <OuterBoundary>
            <Elem name="Basic_BCs">
                <Elem id="1" name="Wall">
                    <Param dtype="REAL" name="Normal_X" value="0.0"/>
                    <Param dtype="REAL" name="Normal_Y" value="0.0"/>
                    <Param dtype="REAL" name="Normal_Z" value="0.0"/>
                    <Param dtype="STRING" name="Specified_Type" value="Velocity"/>
                    <Param dtype="STRING" name="Profile" value="Constant"/>
                    <Param dtype="REAL" name="Specified_Value" value="0.0"/>
                </Elem>
                <Elem id="10" name="Wall">
                    <Param dtype="REAL" name="Normal_X" value="1.0"/>
                    <Param dtype="REAL" name="Normal_Y" value="0.0"/>
                    <Param dtype="REAL" name="Normal_Z" value="0.0"/>
                    <Param dtype="STRING" name="Specified_Type" value="Velocity"/>
                    <Param dtype="STRING" name="Profile" value="Constant"/>
                    <Param dtype="REAL" name="Specified_Value" value="1.0"/>
                </Elem>
                <Elem id="2" name="Specified_Velocity">
                    <Param dtype="REAL" name="Normal_X" value="1.0"/>
                    <Param dtype="REAL" name="Normal_Y" value="0.0"/>
                    <Param dtype="REAL" name="Normal_Z" value="0.0"/>
                    <Param dtype="STRING" name="Specified_Type" value="Velocity"/>
                    <Param dtype="STRING" name="Profile" value="Constant"/>
                    <Param dtype="REAL" name="Specified_Value" value="5.0"/>
                </Elem>
                <Elem id="3" name="Outflow">
                    <Param dtype="STRING" name="Velocity_Type" value="Average"/>
                </Elem>
            </Elem>
            <Elem name="Face_BC">
                <Elem comment="wall" id="1" name="X_minus">
                    <Param id="600" name="Guide_Cell_ID"/>
                </Elem>
                <Elem comment="wall" id="1" name="X_plus">
                    <Param id="600" name="Guide_Cell_ID"/>
                </Elem>
                <Elem comment="wall" id="1" name="Y_minus">
                    <Param id="600" name="Guide_Cell_ID"/>
                </Elem>
                <Elem comment="wall" id="1" name="Y_plus">
                    <Param id="600" name="Guide_Cell_ID"/>
                </Elem>
                <Elem comment="wall" id="1" name="Z_minus">
                    <Param id="600" name="Guide_Cell_ID"/>
                </Elem>
                <Elem comment="slidewall" id="10" name="Z_plus">
                    <Param id="600" name="Guide_Cell_ID"/>
                </Elem>
            </Elem>
        </OuterBoundary>
    </BC_Table>
</SphereConfig>
