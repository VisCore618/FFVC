Output {
  Log {
    Base             = "On"
    Iteration        = "Off"
    Profiling        = "On"
    WallInfo         = "Off"
    Console {
      TemporalType   = "Step"
      Interval       = 1
    }
    History {
      TemporalType   = "Step"
      Interval       = 1
    }
  }

  Data {
    BasicVariables {
      Format         = "sph"
      TemporalType   = "step"
      Interval       = 10
    }
    DerivedVariables {
      Format         = "sph"
      TemporalType   = "step"
      Interval       = 10

      TotalPressure  = "Off"
      Helicity       = "Off"
      Vorticity      = "Off"
      Qcriterion     = "Off"
    }

    AveragedVariables {
      Format        = "sph"
      TemporalType  = "step"
      Interval      = 100
    }
  }
    
  FormatOption {
    SPH {
      GuideOut        = "Without"
      TimeSlice       = "off"
      DirectoryPath   = "hoge"
    }

    PLOT3D {
      filename        = ""
      gridkind        = "single_grid"
      gridmobility    = "immovable"
      stateoftime     = "unsteady"
      setiblankflag   = "off"
      Dimension       = "3D"
      Formattype      = "unformatted"
      Outputxyz       = "off"
      Outputq         = "off"
      Outputfunction  = "off"
      Outputfuncname  = "off"
      Outputfvbnd     = "off"
    }

    VTK {
      Path            = "VTK"
      Prefix          = "data-"
    }
  }
}


Reference {
  Length       = 1.0
  Velocity     = 50.0
  //MassDensity  = 1.1763
  Temperature {
    Base       = 100.0
    Difference = 100.0
  }
  Medium       = "virtualmetal"
  BasePressure = 0.0
}


ReferenceFrame {
  Mode = "Stationary"
}


ShapeApproximation {
  Method = "Binary"
}


SolvingMethod {
  Flow = "FS_C_EE_D_EE"
  Heat = "C_EE_D_EE"
}


StartCondition {
  Restart {
    Staging     = "off"

    DFIfiles {
      Velocity    = "vel0.dfi"
      Pressure    = "prs0.dfi"
      Fvelocity   = "fvel.dfi"
    }
  }

  InitialState {
    MassDensity = 1.0
    Pressure    = 0.0
    Temperature = 150.0
    Velocity    = (0.0, 0.0, 0.0)

    TemperatureOfMedium {
      virtualmetal  = 150.0
      water         = 100.0
    }
  }
}


TimeControl {
  Acceleration {
    TemporalType     = "Time"
    AcceleratingTime = 0.0
  }

  TimeStep {
    Mode           = "direct"
    DeltaT         = 4.0e-4
  }

  Session {
    TemporalType   = "step"
    Start          = 0
    End            = 200
  }

  Average {
    TemporalType   = "step"
    Start          = 0
    End            = 0
  }
}


TreatmentOfWall {
  PressureGradient = "GradZero"
  VelocityProfile  = "NoSlip"
}


TurbulenceModeling {
  Model = "no"
}


Unit {
  UnitOfInputParameter  = "Dimensional"
  UnitOfOutput          = "Dimensional"
  Pressure              = "Gauge"
  Temperature           = "Celsius"
}


MediumTable {
  water {
    State               = "Fluid"
    MassDensity         = 998.2
    SpecificHeat        = 4182.0
    ThermalConductivity = 598.0e-3
    KinematicViscosity  = 1.004e-06
    Viscosity           = 1002.6e-06
    SpeedOFSound        = 1500.0
    VolumeExpansion     = 0.04e-3 
    COLOR               = "F3EB37FF"
  }

  VirtualMetal {
    State               = "Solid"
    MassDensity         = 1.0
    SpecificHeat        = 1.0
    ThermalConductivity = 50.0
    COLOR               = "233C62FF"
  }

} //MediumTable


BcTable {

  LocalBoundary {

    Insulation {
      class  = "adiabatic"
      medium = "virtualmetal"
    }

    Rod {
      class = "HeatTransferB"
      medium = "virtualmetal"
      CoefOfHeatTransfer = 12.0
      BulkTemperature = 100.0
    }

    HeatedWall {
      class = "IsoThermal"
      medium = "virtualmetal"
      temperature = 200.0
    }
  }

  OuterBoundary {
      
    AdiabaticWall {
      class          = "wall"
      Type           = "fixed"
      ThermalOption  = "Adiabatic"
      Medium         = "virtualmetal"
    }
      
    FaceBC {
      Xminus = "AdiabaticWall"
      Xplus  = "AdiabaticWall"
      Yminus = "AdiabaticWall"
      Yplus  = "AdiabaticWall"
      Zminus = "AdiabaticWall"
      Zplus  = "AdiabaticWall"
    }

  }

}

