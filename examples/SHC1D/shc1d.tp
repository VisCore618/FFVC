
ApplicationControl {
  Operator            = "Kenji_Ono"
  FillMedium          = "water"
  HintOfFillingFluid  = "Xminus"
  VoxelOutput     = "svx"
  DebugDivergence = "Off"
}


ConvectionTerm {
  Scheme  = "O2CENTRAL"
}


DomainInfo {
  UnitOfLength   = "NonDimensional"
  GlobalOrigin   = (-0.2, -0.3, -0.3)
  GlobalRegion   = (1.4, 0.6, 0.6)
  GlobalPitch    = (0.2, 0.2, 0.2)
  
  //GlobalDivision = (1    , 1    , 1    )

  ActiveSubDomainFile = "hoge"
}


GoverningEquation {
  FlowEquation       = "Incompressible"
  HeatEquation       = "SolidConduction"
  Buoyancy           = "NoBuoyancy"
  PDEType            = "NavierStokes"
  TimeVariation      = "Unsteady"
}


GeometryModel {
  Source = "polylib.tp"
}


IntrinsicExample {
}


Iteration {
  LinearSolver[@] {
    Alias                = "sor2_moderate"
    class                = "sor2sma"
    MaxIteration         = 50
    ConvergenceCriterion = 1.0e-3
    NormType             = "RbyR0"
    Omega                = 1.1
    CommMode             = "async"
  }

  LinearSolver[@] {
    Alias                = "vp"
    class                = "VPiteration"
    MaxIteration         = 20
    ConvergenceCriterion = 1.0e-4
    NormType             = "VdivMax"
  }

  LinearSolver[@] {
    Alias                = "sor2_loose"
    class                = "sor"
    MaxIteration         = 10
    ConvergenceCriterion = 1.0e-2
    NormType             = "RbyR0"
    Omega                = 1.1
  }

  Pressure    = "sor2_moderate"
  Velocity    = ""
  VPiteration = "vp"
  Temperature = "sor2_loose"   
}


MonitorList {
  Log             = "On"
  OutputMode      = "Gather"
  CellMonitor     = "off"
  Sampling {
    TemporalType  = "step"
    Interval      = 1
  }

